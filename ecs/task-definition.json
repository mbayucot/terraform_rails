{
  "family": "web-family",
  "taskRoleArn": "arn:aws:iam::099459909584:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::099459909584:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "web",
      "image": "099459909584.dkr.ecr.us-east-1.amazonaws.com/rails",
      "cpu": 512,
      "memory": 1024,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
        }
      ],
      "environment": [
        {
          "name": "RAILS_ENV",
          "value": "production"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "demo-app-web",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "demo-web"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024"
}
